#include <stdio.h>
#include <stdlib.h>


void getarray(int array[],int limit,int num);

void display()


int main() {

	int array[100],limit;

getarray();


	return EXIT_SUCCESS;
}



int getarray(int limit,int num,int array[]){


	printf("Enter your value of array:");

	scanf("%d",&limit);

	for(num=0;num<limit;num++){

		scanf("%d",&array[num]);

        display();
	}



}


int display(int num,int limit){


	for(num=0;num<limit;num++){

			printf("%d",num);


		}







}

