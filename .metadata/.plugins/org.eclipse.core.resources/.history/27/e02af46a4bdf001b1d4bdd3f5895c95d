#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int limit,i,j;



	printf(" Enter limit: ");

	scanf("%d",&limit);

     for(i=1;i<=limit;i++){

    	 for(j=1;j<=limit;j++){


    		 printf("*");



    	 }





     }






	return EXIT_SUCCESS;
}
