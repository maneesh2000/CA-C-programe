#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int i,j,number=5;



	for(i=1;i>=1;i++){

		for(j=1;j<=i;j++){



           printf("%d",number);
             number++;


		}


printf("\n");


	}
	return EXIT_SUCCESS;
}
