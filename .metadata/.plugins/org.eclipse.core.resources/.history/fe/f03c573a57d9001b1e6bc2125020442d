#include <stdio.h>
#include <stdlib.h>


void getarray(int array[],int limit,int num);

void display()


int main() {

	int array[100],limit;

getarray(array,limit);
display(array,limit)

	return 0;
}



void getarray(int limit,int num,int array[]){


	printf("Enter your value of array:");

	scanf("%d",&limit);

	for(num=0;num<limit;num++){

		scanf("%d",&array[num]);


	}


}


void display(int num,int limit,int array[]){


	for(num=0;num<limit;num++){

			printf("%d",array[num]);


		}


      printf('\n');


}



