#include <stdio.h>
#include <stdlib.h>

int main(int,int) {

	int array[100],limit;

getarray();


	return EXIT_SUCCESS;
}



void getarray(int limit,int num){


	printf("Enter your value of array:");

	scanf("%d",&limit);

	for(num=0;num<limit;num++){

		scanf("%d",&num);

        display();
	}



}


void display(int num,int limit){


	for(num=0;num<limit;num++){

			printf("%d",num);


		}







}

