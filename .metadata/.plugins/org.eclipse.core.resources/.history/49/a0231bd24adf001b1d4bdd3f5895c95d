#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int limit,i,j,k,l;



	printf(" Enter limit: ");

	scanf("%d",&limit);

     for(i=1;i<=5;i++){

    	 for(j=1;j<=i;j++){


    		 printf("*");



    	 }





     }


     for(i=5-1;i>=1;i++){


    	 for(j=1;j<=i;j++){

    		 printf("*");



    	 }




     }









	return EXIT_SUCCESS;
}
